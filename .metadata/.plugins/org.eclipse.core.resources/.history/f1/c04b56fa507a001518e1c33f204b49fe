package com.umass.hackathon;

public class BinaryOp implements IPredicate {
	String operator;
	String[] operand;
	
	public BinaryOp(String operator, String[] operands) {
		operand = new String[2];
		this.operator = operator;
		this.operand = operand;
	}
	
	public String printClauses() {
		return this.operand[0] + this.operator + this.operand[1];
	}

}
