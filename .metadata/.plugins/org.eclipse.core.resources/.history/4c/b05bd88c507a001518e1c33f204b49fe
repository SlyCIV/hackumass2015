package com.umass.hackathon;

public class UnaryOp implements IClause {
	
	String operator;
	String operand;
	
	public UnaryOp() {
		
	}
	
	public String printClause() {
		switch(this.operator) {
			case "!!":
			case "!":
			case "~":
			case "++pre":
			case "--pre":
				return this.operator + this.operand;
			case "++post":
			case "--post":
				return this.operand + this.operator;
			default:
				return null;
		
		}
	}
}
