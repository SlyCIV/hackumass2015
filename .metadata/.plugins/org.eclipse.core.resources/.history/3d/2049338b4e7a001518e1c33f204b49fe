package com.umass.hackathon;

public class BinaryOp implements IClause {
	String operator;
	String[] operand;
	
	public BinaryOp() {
		operand = new String[2];
		// TODO
	}
	
	public String printClause() {
		// TODO: when wont this be true??? 
		return this.operand[0] + this.operator + this.operand[1];
	}

}
