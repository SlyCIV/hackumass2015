package com.umass.hackathon;

import java.io.BufferedReader;
import java.io.FileReader;

public class Parser {
	
	public String readSentence(String filename) {
		BufferedReader reader;
		
		try {
			reader = new BufferedReader( new FileReader(filename) );
		} catch {
			
		}
		
		
		
		return "1";
	}
}
