	private int findKeywordIndex(String[] s) {
		
		for (int i = 0; i < s.length; i++) {
			switch (s[i]) {
				case "greater": 
				case "less":
				case "equal":
				case "plus":
				case "minus":
				case "times":
				case "divided":
					return 
			}
		}
		
		
		// if not found, return -1;
		return -1;
	}
	
}
